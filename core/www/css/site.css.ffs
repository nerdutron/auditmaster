fieldset
{
	margin-top: 2em;
	background-color: #FFFFFF;
	border: 1px solid #18027C;
}

legend
{
	border-top: 1px solid #18027C;
	border-left: 1px solid #18027C;
	border-right: 1px solid #18027C;
	font-weight: bold;
	padding-left: 1em;
	padding-right: 1em;
	font-size: 1.2em;
	color: #18027C;
}

/* additional colourisations */
.link_highlight { background-color: #DCD1C5; }
.link_highlight a:link, .link_highlight A:visited { color: #000000; }
.alert, .alert ul li, .alert * { color: #CC0000; }



